{
  "name": "drupal/pcb",
  "description": "Permanent Cache Bin module adds support for cache bins which are not cleared during drush cr.",
  "type": "drupal-module",
  "homepage": "https://drupal.org/project/pcb",
  "authors": [
    {
      "name": "Nikunj Kotecha (nikunjkotecha)",
      "homepage": "https://www.drupal.org/u/nikunjkotecha",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://drupal.org/project/issues/pcb",
    "source": "https://cgit.drupalcode.org/pcb"
  },
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1.0",
    "drupal/core": "^10.3 || ^11 || ^12"
  },
  "require-dev": {
    "drupal/memcache": "^2.0",
    "drupal/redis": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\pcb\\": "src/"
    }
  },
  "extra": {
    "drush": {
      "services": {
        "drush.services.yml": "^9 || ^10 || ^11 || ^12 || ^13 || ^14"
      }
    }
  }
}
